import "std.qnp"

void func()...

pack User:
	bool likesProgramming
	u64 age

User tecstylos
tecstylos.likesProgramming = true
tecstylos.age = 17

User* pUser = &tecstylos

print("Age: ")
print(pUser->age)
print("\n")

if tecstylos.likesProgramming:
	print("TecStylos likes programming!\n")
else:
	print("TecStylos doesn't like programming!\n")

exit(0)