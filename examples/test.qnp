import "std.qnp"

pack Location:
	i64 x
	i64 y

pack User:
	u8 name[32]
	bool likesProgramming
	u64 age
	Location loc

void printUser(User* pUser):
	print("User: \n")
	print("  Name: ")
	print(pUser->name)
	print("\n")
	print("  Age: ")
	print(pUser->age)
	print("\n")
	print("  Likes Programming: ")
	if pUser->likesProgramming:
		print("Yes")
	else:
		print("No")
	print("\n")
	print("  Location: ")
	print(pUser->loc.x)
	print(", ")
	print(pUser->loc.y)
	print("\n")

User tecstylos
strcpy(tecstylos.name, "Tecstylos")
tecstylos.likesProgramming = true
tecstylos.age = 17
tecstylos.loc.x = 123
tecstylos.loc.y = 456

User* pUser = &tecstylos

printUser(&tecstylos)

exit(0)