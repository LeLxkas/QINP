import "std.qnp"

pack User:
	u8 name[32]
	bool likesProgramming
	u64 age

void printUser(User* pUser):
	print("User: \n")
	print("  Name: ")
	print(pUser->name)
	print("\n")
	print("  Age: ")
	print(pUser->age)
	print("\n")
	print("  Likes Programming: ")
	if pUser->likesProgramming:
		print("Yes")
	else:
		print("No")
	print("\n")

User tecstylos
strcpy(tecstylos.name, "Tecstylos")
tecstylos.likesProgramming = true
tecstylos.age = 17

User* pUser = &tecstylos

printUser(&tecstylos)

exit(0)