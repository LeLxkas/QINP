union Test:
	i32 a
	i32 b
	i8 c[4]

Test t
t.a = 123456

std::print(t.a)
std::print("\n")

std::print(t.b)
std::print("\n")

std::print((i32)t.c[0])
std::print(", ")
std::print((i32)t.c[1])
std::print(", ")
std::print((i32)t.c[2])
std::print(", ")
std::print((i32)t.c[3])
std::print("\n")

t.c[3] = 12

std::print(t.a)
std::print("\n")

std::print(std::stoi("Fef", 16))
std::print("\n")

bool trueFunc(u8* str):
	std::print(str)
	return true
bool falseFunc(u8* str):
	std::print(str)
	return false

std::print("\n\n")

trueFunc("1\n") && trueFunc("2\n")
trueFunc("3\n") && falseFunc("4\n")
falseFunc("5\n") && falseFunc("6\n")
falseFunc("7\n") && trueFunc("8\n")

std::print("\n\n")

trueFunc("1\n") || trueFunc("2\n")
trueFunc("3\n") || falseFunc("4\n")
falseFunc("5\n") || falseFunc("6\n")
falseFunc("7\n") || trueFunc("8\n")