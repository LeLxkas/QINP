import "std.qnp"

space space1:
	i32 x = 0

	i32 func():
		return 100

space space2:
	i32 x = 2

	i32 func():
		return 200

std::print(::space1::x)
std::print("\n")
::space1::x = 100
std::print(space1::x)
std::print("\n")

std::print(space2::x)
std::print("\n")

std::print(space1::func())
std::print("\n")
std::print(space2::func())
std::print("\n")