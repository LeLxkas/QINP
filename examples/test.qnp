import "std.qnp"

pack Location:
	i64 x
	i64 y
	i64 z

Location makeLocation(i64 x, i64 y, i64 z):
	Location loc
	loc.x = x
	loc.y = y
	loc.z = z
	return loc

void printLocation(Location loc, i64 locx):
	print("Location: ")
	print(loc.x)
	print(", ")
	print(loc.y)
	print(", ")
	print(loc.z)
	print("\n")
	print("LocX: ")
	print(locx)
	print("\n")

Location l
l = makeLocation(1, 2, 3)
printLocation(l, 111)

