\\ QINP Standard Library
\\ Linux Specific

import "../../fileio.qnp"

space std:
	define __STDIN (File*)0
	define __STDOUT (File*)1
	define __STDERR (File*)2



defer
space std:
	i32 printn(u8 const* str, u64 num)!:
		return fwrite(__STDOUT, str, num)

	u8 getchar()!:
		u8 ch
		u64 result = fread(__STDIN, &ch, 1)
		return ch